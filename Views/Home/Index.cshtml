@model List<QueueModel>

<table class="table table-dark shadow mt-5 mb-3 border border-5 border-light rounded-3">
    <thead>
        <tr>
            <th scope="col">Ism Sharifingiz</th>
            <th scope="col">Telefon Raqamingiz</th>
            <th scope="col">Navbat olgan vaqtingiz</th>
            <th scope="col">Navbatingiz keladigan vaqt</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.CustomerName</td>
                <td>(XX) XXX XX @user.Phone.Substring(user.Phone.Length - 2)</td>
                <td>@user.CreatedAt.ToString("MMM dd, yyyy HH:mm")</td>
                <td>@user.QueueTime.ToString("MMM dd, yyyy HH:mm")</td>
            </tr>
        }

    </tbody>
</table>
